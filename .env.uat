# UAT 环境配置
NEXT_PUBLIC_ENV=uat

# 端口配置
PORT=4003

# API 配置
NEXT_PUBLIC_API_BASE_URL=https://uat-api.yourdomain.com/api

# Qwen API 配置
NEXT_PUBLIC_QWEN_API_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
NEXT_PUBLIC_QWEN_API_KEY=your-uat-api-key-here
NEXT_PUBLIC_QWEN_MODEL=qwen-plus
NEXT_PUBLIC_QWEN_SYSTEM_PROMPT=You are a helpful assistant.
NEXT_PUBLIC_QWEN_DIRECT_PROXY=false
